    "@types/lodash": "^4.14.177",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.1.2",
    "typescript": "^4.3.5"
