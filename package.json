{
  "name": "employee-expressjs",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41"
  },
  "scripts": {
    "first_start": "ts-node initializeDB.ts && npm run start",
    "start": "ts-node --type-check src/app.ts JWT_KEY:PRIVATE_KEY",
